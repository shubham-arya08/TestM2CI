{
    "name": "vertexinc/request-logging-api-module",
    "version": "1.0.1",
    "description": "Vertex Request Logging module for Magento 2 API",
    "type": "magento2-module",
    "require": {
        "magento/framework": "^100|^101|^102|^103"
    },
    "require-dev": {
        "mediotype/code-standard": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "@stable",
        "jetbrains/phpstorm-attributes": "^1"
    },
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Vertex\\RequestLoggingApi\\": ""
        }
    },
    "archive": {
        "exclude": [
            ".github/",
            "composer.lock",
            "phpcs-new.xml",
            "phpcs-old.xml",
            "vendor/",
            ".git/",
            ".gitignore"
        ]
    }
}
