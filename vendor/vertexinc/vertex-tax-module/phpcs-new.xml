<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Vertex.Tax">
    <file>.</file>
    <exclude-pattern>vendor/*</exclude-pattern>
    <exclude-pattern>artifact/*</exclude-pattern>
    <rule ref="Mediotype"/>
    <rule ref="Mediotype.PHP.FileHeader">
        <properties>
            <property name="forceCurrentYear" value="true"/>
            <property name="codeOwner" value="Vertex, Inc"/>
        </properties>
    </rule>
</ruleset>
